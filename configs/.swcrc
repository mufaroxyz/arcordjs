{
    "$schema": "https://json.schemastore.org/swcrc",
    "module": {
      "type": "es6",
      "strict": false,
      "lazy": false,
      "noInterop": false
    },
    "jsc": {
      "parser": {
        "syntax": "typescript",
        "dynamicImport": false,
        "decorators": false,
        "dts": true
      },
      "target": "esnext",
      "loose": false,
      "externalHelpers": false,
      "keepClassNames": false
    },
    "minify": false
  }